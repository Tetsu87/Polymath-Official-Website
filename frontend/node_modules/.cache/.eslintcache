ge, options } = e.target.elements;\n\n    let details = {\n      name: name.value,\n      email: email.value,\n      tel: tel.value,\n      message: message.value,\n      options: options.value,\n    };\n\n    let response = await fetch(\n      \"https://polymath-express.wl.r.appspot.com/contact\",\n      {\n        method: \"POST\",\n        headers: {\n          \"Content-Type\": \"application/json;charset=utf-8\",\n        },\n        body: JSON.stringify(details),\n      }\n    );\n    setStatus(\"Submit\");\n    let result = await response.json();\n    alert(result.status);\n  };\n\n  return (\n    <form onSubmit={handleSubmit}>\n      <div className=\"form-wrapper\">\n        <MediaQuery query=\"(max-width: 767px)\">\n          <p className=\"small-letter smartphone-contact\">\n            無料相談を随時承ってます。まずはお気軽にご連絡ください。\n          </p>\n          <div className=\"mb-1\">\n            <input\n              type=\"text\"\n              id=\"name\"\n              placeholder=\"名前\"\n              className=\"smartphone-contact\"\n              required\n            />\n          </div>\n          <div className=\"mb-1\">\n            <input\n              type=\"email\"\n              id=\"email\"\n              placeholder=\"メールアドレス\"\n              className=\"smartphone-contact\"\n              required\n            />\n          </div>\n          <div className=\"mb-1\">\n            <input\n              type=\"text\"\n              id=\"tel\"\n              placeholder=\"電話番号\"\n              className=\"smartphone-contact\"\n              required\n            />\n          </div>\n          <div className=\"mb-1\">\n            <textarea\n              id=\"message\"\n              placeholder=\"問い合わせ内容\"\n              className=\"smartphone-contact\"\n              required\n            ></textarea>\n          </div>\n          <div>\n            <select\n              id=\"options\"\n              required\n              className=\"mt-3 option-font smar